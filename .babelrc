{
    "presets": [
      ["@babel/preset-env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }],
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/plugin-transform-runtime",{
        "corejs": 2, 
        "useBuildIns":"usage" 
      }],"@babel/plugin-proposal-class-properties",
      ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true 
      }]
    ]
  }